{
    "id": "bbtc",
    "title": "Соревнование Английский Бульдог",
    "users": {
        "fields": [
            {
                "name": "login",
                "input": {
                    "type": "string",
                    "required": true,
                    "validator": {
                        "type": "login"
                    }
                }
            },
            {
                "name": "surname",
                "input": {
                    "type": "string",
                    "required": true
                }
            },
            {
                "name": "name",
                "input": {
                    "type": "string",
                    "required": true
                }
            },
            {
                "name": "patronymic",
                "input": {
                    "type": "string",
                    "required": true
                }
            },
            {
                "name": "birthday",
                "input": {
                    "type": "date"
                }
            },
            {
                "name": "phone",
                "input": {
                    "type": "string"
                }
            },
            {
                "name": "email",
                "input": {
                    "type": "string",
                    "required": true,
                    "validator": {
                        "type": "email"
                    }
                }
            },
            {
                "name": "schoolcode",
                "input": {
                    "type": "string",
                    "required": true,
                    "validators": [
                        {
                            "type": "pattern",
                            "pattern": "\\d{8}",
                            "message": "error.msg.schoolcode.wrong_pattern"
                        },
                        {
                            "type": "kenguruSchoolCode"
                        }
                    ]
                }
            },
            {
                "name": "schoolname",
                "input": {
                    "type": "string",
                    "required": true
                }
            }
        ],
        "validators": []
    }
}